{
  "manifest_version": 3,
  "name": "Mentimeter CSV Presentation Scraper",
  "version": "0.1.0",
  "action": { "default_popup": "popup.html" },
  "background": { "service_worker": "background.js" },
  "permissions": ["downloads"],
  "host_permissions": [
    "https://www.mentimeter.com/*",
    "https://mentimeter.com/*",
    "https://api.mentimeter.com/*"
  ]
}